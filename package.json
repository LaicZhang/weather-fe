{
  "name": "weather-fe",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host --open",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "serve": "serve -s dist",
    "lint:script": "eslint . --fix",
    "lint:style": "stylelint --fix \"src/**/*.(vue|less|css)\" --customSyntax postcss-less",
    "lint": "pnpm run lint:script && pnpm run lint:style",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^8.6.0",
    "axios": "^0.27.2",
    "echarts": "^5.3.2",
    "element-plus": "~2.1.11",
    "rollup": "2.72.0",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.7.0",
    "vite": "^2.9.9",
    "vite-plugin-chunk-split": "^0.2.2",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15",
    "vue-simple-password-meter": "^1.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.2.0",
    "@antfu/eslint-config": "^0.25.0",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@iconify/json": "^2.1.20",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^2.3.2",
    "autoprefixer": "^10.4.2",
    "commitlint": "^16.2.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "less": "^3.0.4",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "postcss-import": "14.0.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vitest": "^0.28.4"
  },
  "husky": {
    "pre-commit": "npm run lint"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,vue,json,md,html}": [
      "pnpm run lint:script",
      "git add --force"
    ],
    "**/*.{scss,vue,css}": [
      "pnpm run lint:style",
      "git add --force"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
