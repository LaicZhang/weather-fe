{
  "name": "weather-fe",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host --open",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "serve": "serve -s dist",
    "lint:script": "eslint . --fix",
    "lint:style": "stylelint --fix \"src/**/*.(vue|less|css)\" --customSyntax postcss-less",
    "lint": "pnpm run lint:script && pnpm run lint:style",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@types/lodash-es": "^4.17.8",
    "@vueuse/core": "^8.6.0",
    "axios": "^0.27.2",
    "echarts": "^5.4.3",
    "element-plus": "~2.3.10",
    "rollup": "2.79.1",
    "stylelint-prettier": "^3.0.0",
    "ts-node": "^10.7.0",
    "vite": "^2.9.16",
    "vite-plugin-chunk-split": "^0.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-simple-password-meter": "^1.3.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.2.0",
    "@antfu/eslint-config": "^0.40.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.102",
    "@types/node": "^20.5.6",
    "@vitejs/plugin-vue": "^2.3.2",
    "autoprefixer": "^10.4.2",
    "commitlint": "^17.7.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.27",
    "postcss-html": "^1.4.1",
    "postcss-import": "15.1.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.65.1",
    "stylelint": "^14.8.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vitest": "^0.34.1"
  },
  "husky": {
    "pre-commit": "npm run lint"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,vue,json,md,html}": [
      "pnpm run lint:script",
      "git add --force"
    ],
    "**/*.{scss,vue,css}": [
      "pnpm run lint:style",
      "git add --force"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
